void main()
{
    int a,b,c;	
    if(c<>1){
     READ(c);
       c = (((a-1)*(a*10))*a);  /* nested Arithmetic*/ 
    }
    else
    {
     READ(a);
    }
    while(a <> b){	
        if(a>b){	
            a=a-b;
        }
        else{
            b=b-a;
        }
    }
    WRITE(a);
    /*printf("GCD of given two numbers is %d",a);*/
}
